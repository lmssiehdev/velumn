{
  "$schema": "https://biomejs.dev/schemas/2.2.2/schema.json",
  "extends": ["ultracite"],
  "files": {
    "includes": ["!**/*.{jsx,tsx}"]
  },
  "linter": {
    "rules": {
      "complexity": {
        "useSimplifiedLogicExpression": "off"
      },
      "suspicious": {
        "noConsole": "off"
      }
    }
  }
}
